{
  "parameters": [
    "duration",
    "delay",
    "easing",
    "componentId"
  ],
  "commands": [
      {
      "type": "AnimateItem",
      "componentId": "${componentId}",
      "duration": "${duration || 200}",
      "delay": "${delay || 0}",
      "easing": "${easing || ease}",
      "value": [
        {
          "property": "opacity",
          "from": 1,
          "to": 0
        }
      ]
    }
  ]
}
